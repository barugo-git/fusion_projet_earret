{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/annotations": "^1.13.1",
        "doctrine/dbal": "^3.3",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.13",
        "dompdf/dompdf": "*",
        "fedapay/fedapay-php": "0.4.5",
        "infobip/infobip-api-php-client": "^3.2",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpoffice/phpword": "^1.3",
        "phpstan/phpdoc-parser": "^1.13",
        "ramsey/uuid": "^4.7",
        "setasign/fpdi": "^2.6",
        "smalot/pdfparser": "*",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "7.0.*",
        "symfony/asset-mapper": "7.1.*",
        "symfony/console": "7.0.*",
        "symfony/doctrine-messenger": "^5.1|^6.0",
        "symfony/dotenv": "7.0.*",
        "symfony/expression-language": "7.0.*",
        "symfony/flex": "^2",
        "symfony/form": "7.0.*",
        "symfony/framework-bundle": "7.0.*",
        "symfony/google-mailer": "^6",
        "symfony/http-client": "7.0.*",
        "symfony/intl": "5.4.*",
        "symfony/mailer": "7.0.*",
        "symfony/mime": "7.0.*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/notifier": "5.4.*",
        "symfony/process": "^6",
        "symfony/property-access": "6.4.*",
        "symfony/property-info": "6.4.*",
        "symfony/proxy-manager-bridge": "^6",
        "symfony/routing": "7.0.*",
        "symfony/runtime": "7.0.*",
        "symfony/security-bundle": "7.1.*",
        "symfony/serializer": "7.0.*",
        "symfony/string": "^6.4|^7.0",
        "symfony/translation": "7.0.*",
        "symfony/twig-bundle": "7.0.*",
        "symfony/uid": "7.1.*",
        "symfony/validator": "6.4.*",
        "symfony/web-link": "5.4.*",
        "symfony/yaml": "7.0.*",
        "symfonycasts/reset-password-bundle": "^1.23",
        "symfonycasts/verify-email-bundle": "^1.12",
        "tecnickcom/tcpdf": "*",
        "twig/cssinliner-extra": "^3.4",
        "twig/extra-bundle": "^3.4",
        "twig/intl-extra": "^3.12",
        "twig/twig": "^2.12|^3.0",
        "twilio/sdk": "^6.43",
        "vich/uploader-bundle": "*"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "platform": {
            "php": "8.2.0"
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.1.*"
        }
    },
    "require-dev": {
        "dbrekelmans/bdi": "^1.3",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^1.1",
        "symfony/browser-kit": "7.0.*",
        "symfony/css-selector": "5.4.*",
        "symfony/debug-bundle": "7.0.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/panther": "^2.1",
        "symfony/phpunit-bridge": "^7",
        "symfony/stopwatch": "7.0.*",
        "symfony/var-dumper": "7.0.*",
        "symfony/web-profiler-bundle": "7.0.*"
    }
}
